{
  "manifest_version": 3,
  "name": "Verbose Notetaker",
  "description": "Record and transcribe audio in realtime",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ],
  "permissions": [
    "desktopCapture",
    "system.display"
  ]
}